    "surfaces": {
        "in_app_panel": {
            "format": "dva",
            "version": "1",
            "type": "project",
            "additionalAssets": [
                "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/create-trippy-transitions/IMG_4005.mp4",
                "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/create-trippy-transitions/IMG_4124.mp4"
            ],
            "minVersion": "15.0",
            "projectUrl": "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/create-trippy-transitions/Jillian-Nicole-Smith_TikTok3.prproj",
            "sizeBytes": "123456789",
            "steps": [
                {
                    "stepItems": [
                        {
                            "autoplay": true,
                            "type": "video",
                            "videoUrl": "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/create-trippy-transitions/10e408d8-8447-4f1d-8a95-e4879d9a6084.mp4"
                        },
                        {
                            "text": "Create a boxed-in visual effect",
                            "type": "heading"
                        },
                        {
                            "text": "Start with a clip of me looking confused and a clip of me holding my phone. By masking the image of me onto the phone, I appear to be trapped inside my phone.",
                            "type": "text"
                        },
                        {
                            "listItems": [
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "isCtiOverClip",
                                            "payload": {
                                                "clipName": "IMG_4124.mp4"
                                            }
                                        }
                                    ],
                                    "text": "In the <$Timeline$> panel, move the playhead over the second clip (my face), so it appears in the <$Program Monitor$>."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "clipAdded",
                                            "payload": {
                                                "clipName": "IMG_4005.mp4",
                                                "layerName": "Video 2"
                                            }
                                        }
                                    ],
                                    "text": "Drag the first clip (the phone) to the <$V2$> track so it's directly above the second clip. Keep the upper clip selected."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "isPanelOpen",
                                            "payload": {
                                                "panelName": "Effect Controls"
                                            }
                                        }
                                    ],
                                    "text": "Open the <$Effect Controls$> panel by choosing <$Window$> > <$Effect Controls$>."
                                },
                                {
                                    "text": "In the <$Opacity$> section of the <$Effect Controls$> panel, click the rectangle icon to add a rectangular mask to the upper clip. The lower clip's footage appears outside it."
                                },
                                {
                                    "text": "In the <$Mask (1)$> section under <$Opacity$>, click <$Inverted$> to let my face pop through."
                                },
                                {
                                    "text": "In the <$Program Monitor$>, drag the four corners of the mask so they overlap the corners of the phone screen."
                                },
                                {
                                    "text": "In the <$Mask (1)$> section, increase <$Mask Expansion$> to about <$60$> so that the rounded corners of the mask match the rounded corners of the phone more closely."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "ctiStopped"
                                        },
                                        {
                                            "check": "ctiMoved"
                                        }
                                    ],
                                    "showOnComplete": {
                                        "heading": "Almost there!",
                                        "texts": [
                                            "Because the position of the mask is fixed, it fails to follow the handheld motion of the phone, breaking the illusion. In the next section, you learn how to move the mask with the footage."
                                        ]
                                    },
                                    "text": "Scrub the playhead to see the masked footage."
                                }
                            ],
                            "type": "listSubsteps"
                        }
                    ]
                },
                {
                    "stepItems": [
                        {
                            "autoplay": true,
                            "type": "video",
                            "videoUrl": "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/create-trippy-transitions/13fe3b2f-bcd5-412f-b1e2-4d40e7a61968.mp4"
                        },
                        {
                            "text": "Bring the effect to life",
                            "type": "heading"
                        },
                        {
                            "text": "Animate the mask using keyframes so the movement feels totally natural. It's a cool way to transition from shot to shot.",
                            "type": "text"
                        },
                        {
                            "listItems": [
                                {
                                    "text": "Move the playhead to the beginning of the clips. Make sure the top clip is still selected."
                                },
                                {
                                    "text": "In the <$Mask (1)$> section of the <$Effect Controls$> panel, click the stopwatch next to <$Mask Path$> to start animating it."
                                },
                                {
                                    "text": "Click <$Mask (1)$> to display the mask in the <$Program Monitor$>."
                                },
                                {
                                    "text": "Adjust the four blue points of the mask so they fit the contour of the phone's screen."
                                },
                                {
                                    "text": "In the <$Timeline$> panel, enter <$+3$> or <$+5$> in the timecode indicator to advance the playhead by that many frames."
                                },
                                {
                                    "text": "Readjust the blue points of the mask so the rounded corners fit the phone. Keyframes are recorded at the new <$Mask Path$> positions."
                                },
                                {
                                    "text": "Continue advancing the playhead 3–5 frames and adjusting the mask corners each time so they stay with the phone. (Drag the mask's center if you want to reposition it.) The more precisely you adjust the mask for the duration of the clip, the better the result."
                                },
                                {
                                    "text": "Play the sequence. The illusion is now maintained throughout the clip."
                                }
                            ],
                            "type": "listSubsteps"
                        }
                    ]
                },
                {
                    "stepItems": [
                        {
                            "successHeading": "Voilà!",
                            "successText": "Your trippy transition is now ready for TikTok.",
                            "type": "endScreen"
                        }
                    ]
                }
            ]
        },
