            "sizeBytes": "123456789",
            "steps": [
                {
                    "stepActionsOnShow": [
                        {
                            "actionName": "openProjectAndCloseAndSaveOthers",
                            "payload": {
                                "projectUrl": "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/make-adventure-videos/KarlShakur_transition.prproj"
                            }
                        }
                    ],
                    "stepItems": [
                        {
                            "autoplay": true,
                            "type": "video",
                            "videoUrl": "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/make-adventure-videos/ac5b41d8-1e60-420b-a257-d435bb024ede.mp4"
                        },
                        {
                            "text": "Layer your footage",
                            "type": "heading"
                        },
                        {
                            "text": "To start things out, arrange the clips so the special transition effect I want to show you will work properly. The clip that starts the transition should be above the clip that ends the transition.",
                            "type": "text"
                        },
                        {
                            "listItems": [
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "clipAdded",
                                            "payload": {
                                                "clipName": "Door",
                                                "layerName": "Video 2"
                                            }
                                        }
                                    ],
                                    "text": "Drag the <$Door$> clip from the <$Project$> panel to the <$V2$> track in the <$Timeline$> panel."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "clipAdded",
                                            "payload": {
                                                "clipName": "Trees",
                                                "layerName": "Video 1"
                                            }
                                        }
                                    ],
                                    "text": "Drag the <$Trees$> clip to the <$V1$> track in the <$Timeline$> panel so it's below the <$Door$> clip."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "ctiStopped"
                                        },
                                        {
                                            "check": "ctiMoved"
                                        },
                                        {
                                            "check": "ctiStopped"
                                        }
                                    ],
                                    "showOnComplete": {
                                        "heading": "Success!",
                                        "texts": [
                                            "You layered the clips in the <$Timeline$> panel so they're ready for you to apply the special transition effect. Next you'll add a mask to the upper clip so the lower clip shows through it."
                                        ]
                                    },
                                    "text": "Press the <%{KEY.SPACEBAR}%> to play the sequence all the way to the end. The <$Trees$> clip appears only after the <$Door$> clip ends."
                                }
                            ],
                            "type": "listSubsteps"
                        }
                    ]
                },
                {
                    "stepItems": [
                        {
                            "autoplay": true,
                            "type": "video",
                            "videoUrl": "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/make-adventure-videos/aae4a98f-9782-46a1-9506-9a762a9502fc.mp4"
                        },
                        {
                            "text": "Apply a mask",
                            "type": "heading"
                        },
                        {
                            "text": "Here you add a mask to the clip in the upper track so it reveals the clip in the lower track.",
                            "type": "text"
                        },
                        {
                            "listItems": [
                                {
                                    "text": "Move the playhead to about <$02;45$> in the <$Timeline$> panel."
                                },
                                {
                                    "text": "In the <$Program Monitor$>, change the Zoom level (try <$25%$> or <$50%$>) so there's more area around the video."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "isClipSelected",
                                            "payload": {
                                                "clipName": "Door",
                                                "layerName": "Video 2"
                                            }
                                        }
                                    ],
                                    "text": "In the <$Timeline$> panel, select the <$Door$> clip in the <$V2$> track."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "isPanelOpen",
                                            "payload": {
                                                "panelName": "Effect Controls"
                                            }
                                        }
                                    ],
                                    "text": "Open the <$Effect Controls$> panel by choosing <$Window$> > <$Effect Controls$>."
                                },
                                {
                                    "text": "In the <$Opacity$> section of the <$Effect Controls$> panel, click the pen icon to activate the <$Free Draw Bezier$> tool."
                                },
                                {
                                    "text": "In the <$Program Monitor$>, click to draw mask points around the doorway opening. After you complete the mask path, the lower clip's footage will appear outside it."
                                },
                                {
                                    "text": "At the bottom of the <$Mask (1)$> section of the <$Effect Controls$> panel, click <$Inverted$> to let the lower clip's footage show through the mask."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "ctiStopped"
                                        },
                                        {
                                            "check": "ctiMoved"
                                        }
                                    ],
                                    "showOnComplete": {
                                        "heading": "Almost there!",
                                        "texts": [
                                            "The transition effect requires the mask to change as the door opens. In the next section, I show you how to animate the mask so it'll do just that."
                                        ]
                                    },
                                    "text": "Scrub the playhead to see the masked footage."
                                }
                            ],
                            "type": "listSubsteps"
                        }
                    ]
                },
                {
                    "stepItems": [
                        {
                            "autoplay": true,
                            "type": "video",
                            "videoUrl": "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/make-adventure-videos/78169fda-0c97-4274-850a-b7062357ac1e.mp4"
                        },
                        {
                            "text": "Animate your mask",
                            "type": "heading"
                        },
                        {
                            "text": "In this section, you adjust the mask over time to seamlessly reveal the underlying footage throughout the action.",
                            "type": "text"
                        },
                        {
                            "listItems": [
                                {
                                    "text": "Return the playhead to about <$02;45$> in the <$Timeline$> panel and make sure the <$Door$> clip in the <$V2$> track is selected."
                                },
                                {
                                    "text": "In the <$Mask (1)$> section of the <$Effect Controls$> panel, click the stopwatch next to <$Mask Path$> to indicate you want to change the mask's shape over time."
                                },
                                {
                                    "text": "Click <$Mask (1)$> to display the mask's blue border in the <$Program Monitor$>."
                                },
                                {
                                    "text": "In the <$Program Monitor$>, click the <$Step Forward 1 Frame$> button (to the right of the <$Play$> button) and adjust the mask to fit the larger doorway opening. Repeat this step, one frame at a time, until the mask covers the viewable area and the lower clip fills the frame."
                                },
                                {
                                    "text": "Return the playhead to about <$02;45$> in the <$Timeline$> panel, or wherever you started keyframing the mask path."
                                },
                                {
                                    "text": "Click the <$Step Back 1 Frame$> button and adjust the mask to fit the smaller doorway opening. Repeat this step until the mask is very small and you see barely any footage from the lower clip."
                                },
                                {
                                    "text": "Move your mouse over the mask until you see a hand icon and drag the mask off the viewable area."
                                },
                                {
                                    "text": "Choose <$Sequence$> > <$Render In to Out$> to view a smooth playback of the mask animation."
                                }
                            ],
                            "type": "listSubsteps"
                        },
                        {
                            "text": "When you're done, you'll have animated a mask so it follows the action in the shot, gradually completing the seamless transition effect. All you need to do now is replace the practice footage with a different shot.",
                            "type": "text"
                        }
                    ]
                },
                {
                    "stepItems": [
                        {
                            "autoplay": true,
                            "type": "video",
                            "videoUrl": "https://helpx.adobe.com/content/dam/help/en/premiere-pro/how-to/in-app/make-adventure-videos/8f9c7cd0-c0e3-421f-aec8-a82e6a1fbe50.mp4"
                        },
                        {
                            "text": "Mix it up",
                            "type": "heading"
                        },
                        {
                            "text": "See how easy it is to perfect the effect and swap in your own footage.",
                            "type": "text"
                        },
                        {
                            "listItems": [
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "isClipSelected",
                                            "payload": {
                                                "clipName": "Door",
                                                "layerName": "Video 2"
                                            }
                                        }
                                    ],
                                    "text": "Make sure the <$Door$> clip in the <$V2$> track is selected."
                                },
                                {
                                    "text": "In the <$Mask (1)$> section of the <$Effect Controls$> panel, increase <$Mask Feather$> to about <$20$> and increase <$Mask Expansion$> to about <$10$>."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "clipRemoved",
                                            "payload": {
                                                "clipName": "Trees",
                                                "layerName": "Video 1"
                                            }
                                        }
                                    ],
                                    "text": "Select the <$Trees$> clip in the <$V1$> track and press <%{KEY.DELETE}%>."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "clipAdded",
                                            "payload": {
                                                "clipName": "Rio",
                                                "layerName": "Video 1"
                                            }
                                        }
                                    ],
                                    "text": "Drag the <$Rio$> clip from the <$Project$> panel to the <$V1$> track so it's below the <$Door$> clip."
                                },
                                {
                                    "checkUserInteractions": [
                                        {
                                            "check": "ctiStopped"
                                        },
                                        {
                                            "check": "ctiMoved"
                                        },
                                        {
                                            "check": "ctiStopped"
                                        }
                                    ],
                                    "showOnComplete": {
                                        "heading": "Perfect!",
                                        "texts": [
                                            "You swapped the sample footage with a different clip."
                                        ]
                                    },
                                    "text": "Choose <$Sequence$> > <$Render In to Out$> to view the transition effect on the added clip. Play the sequence to the very end."
                                }
                            ],
                            "type": "listSubsteps"
                        }
                    ]
                },
                {
                    "stepItems": [
                        {
                            "successHeading": "Congratulations!",
                            "successText": "Now that you've created a seamless edit between our two sample clips, wow your viewers with a mask that follows some action across the screen to transition seamlessly between two shots of your own.",
                            "type": "endScreen"
                        }
                    ]
                }
            ]
        },
